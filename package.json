{
  "name": "homequest-clone",
  "description": "An Aurelia 2 client application.",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "???"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "aurelia": "dev"
  },
  "devDependencies": {
    "@aurelia/plugin-gulp": "dev",
    "@aurelia/testing": "dev",
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.12.1",
    "autoprefixer": "10.0.1",
    "babel-eslint": "10.1.0",
    "browser-do": "3.0.2",
    "connect": "3.7.0",
    "connect-history-api-fallback": "1.6.0",
    "connect-injector": "0.4.4",
    "cross-env": "7.0.2",
    "cypress": "5.4.0",
    "del": "6.0.0",
    "es-abstract": "next",
    "eslint": "7.12.1",
    "eslint-plugin-cypress": "2.11.2",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-dart-sass": "1.0.2",
    "gulp-dumber": "1.0.3",
    "gulp-if": "3.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.0",
    "gulp-run": "1.7.1",
    "gulp-terser": "1.4.0",
    "htmlhint": "0.14.1",
    "merge2": "1.4.1",
    "open": "7.3.0",
    "postcss": "^8.1.4",
    "postcss-url": "10.0.0",
    "sass-lint": "1.13.1",
    "serve-static": "1.14.1",
    "socket.io": "2.3.0",
    "start-server-and-test": "1.11.5",
    "tape": "5.0.1"
  },
  "scripts": {
    "lint:js": "eslint src test cypress",
    "lint:css": "sass-lint -c .sass-lint.yml \"src/**/*.scss\"",
    "lint:html": "htmlhint -c .htmlhintrc src",
    "lint": "npm run lint:js && npm run lint:html && npm run lint:css",
    "test": "npm run test:headless",
    "pretest": "npm run lint && npm run build:test",
    "start": "gulp",
    "build": "gulp clean && cross-env NODE_ENV=production gulp build",
    "clear-cache": "gulp clear-cache",
    "build:test": "gulp clean && cross-env NODE_ENV=test gulp build",
    "test:watch": "gulp clean && cross-env NODE_ENV=test gulp watch-test",
    "test:headless": "browser-do --tap < dist/entry-bundle.js",
    "test:chrome": "browser-do --tap --browser chrome < dist/entry-bundle.js",
    "cypress": "cypress run",
    "test:e2e": "cross-env CI=1 start-server-and-test start http://localhost:9000 cypress"
  }
}
